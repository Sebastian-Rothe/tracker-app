{
  "expo": {
    "name": "Routine Tracker",
    "slug": "tracker",
    "version": "1.0.0",
    "description": "Track your daily routines with simple streak counters and daily reminders",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher/android/mipmap-xxxhdpi/ic_launcher.png",
    "scheme": "morgenroutine-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "githubUrl": "https://github.com/Sebastian-Rothe/tracker-app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.routinetracker.app",
      "buildNumber": "1",
      "minimumOsVersion": "13.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher/android/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.routinetracker.app",
      "versionCode": 1,
      "permissions": ["SCHEDULE_EXACT_ALARM", "RECEIVE_BOOT_COMPLETED"]
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ic_launcher/android/mipmap-xxxhdpi/ic_launcher.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ic_launcher/android/mipmap-xxxhdpi/ic_launcher.png",
          "color": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cdabbcb6-2d8c-4bd7-9a75-a995258d2fa6"
      }
    },
    "owner": "rsproduction"
  }
}
